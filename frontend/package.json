{
  "name": "aplose-frontend",
  "private": true,
  "version": "2.0.0-1",
  "type": "module",
  "homepage": ".",
  "proxy": "http://localhost:8000",
  "scripts": {
    "dev": "concurrently \"npm run graphql:generate\" \"vite\"",
    "wait:all": "wait-on http://localhost:8000/backend/admin && wait-on http://localhost:5173/app",
    "wait:server": "wait-on http://localhost:8000/backend/admin",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest",
    "test:playwright": "concurrently --kill-others -s=first \"npm run dev\" \"npx playwright test -c ./playwright.config.ts ./tests\"",
    "docs:dev": "vitepress dev docs --base \"/doc/\"",
    "docs:build": "vitepress build docs --base \"/doc/\"",
    "docs:preview": "vitepress preview docs",
    "graphql:generate": "npx graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@codemirror/commands": "^6.8.1",
    "@codemirror/lang-sql": "^6.8.0",
    "@codemirror/view": "^6.36.5",
    "@ionic/react": "^7.6.2",
    "@reduxjs/toolkit": "^2.2.1",
    "@rtk-query/graphql-request-base-query": "^2.3.1",
    "@xyflow/react": "^12.7.0",
    "bootstrap": "^5.3.2",
    "codemirror": "^6.0.1",
    "graphql-request": "^6.1.0",
    "ionicons": "^7.2.2",
    "react-bootstrap": "^2.9.1",
    "react-confirm": "^0.3.0-7",
    "react-icons": "^5.3.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^7.5.1",
    "rxjs": "^7.8.1",
    "sass": "^1.77.8",
    "signal-ts": "^1.2.4",
    "superagent": "^8.1.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/typescript-resolvers": "^4.5.1",
    "@graphql-codegen/typescript-rtk-query": "^3.1.2",
    "@graphql-codegen/near-operation-file-preset": "^3.1.0",
    "@jest/globals": "^29.7.0",
    "@parcel/watcher": "^2.5.1",
    "@playwright/test": "^1.48.2",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.13",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/redux-mock-store": "^1.0.6",
    "@types/superagent": "^4.1.21",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-test-renderer": "^18.3.1",
    "redux-mock-store": "^1.5.5",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "vitepress": "^1.5.0",
    "wait-on": "^8.0.1"
  }
}
